{
    "name": "localdatasharing",
    "main": "src/worker.js",
    "compatibility_date": "2024-09-23",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "directory": "./public",
        "binding": "ASSETS"
    },
    "r2_buckets": [
        {
            "binding": "MY_BUCKET",
            "bucket_name": "localdata"
        }
    ],
    "observability": {
        "enabled": true
    }
}